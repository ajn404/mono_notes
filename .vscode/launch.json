{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "pnpm  -F @notes/docs dev",
      "name": "run docs",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "pnpm run cz",
      "name": "commit-cz",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/ajn404-vscode-extension"
      ]
    }
  ]
}
